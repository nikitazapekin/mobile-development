<?xml version="1.0" encoding="utf-8"?>
<!--suppress NavigationFile -->
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/main2">

    <fragment
        android:id="@+id/second"
        android:name="com.example.navig.Second"
        android:label="fragment_second"
        tools:layout="@layout/fragment_second">
        <argument
            android:name="name"
            app:argType="string" />

        <argument
            android:name="telephone"
            app:argType="string" />

        <argument
            android:name="adres"
            app:argType="string" />

        <argument
            android:name="savedItem"
            app:argType="string[]" />



        <action
            android:id="@+id/action_second_to_makeOrder"
            app:destination="@id/makeOrder" />
    </fragment>
    <fragment
        android:id="@+id/main2"
        android:name="com.example.navig.Main"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_main2_to_second"
            app:destination="@id/second" >


        <argument android:name="name"
            app:argType="string"
            />

        </action>
    </fragment>
    <fragment
        android:id="@+id/makeOrder"
        android:name="com.example.navig.MakeOrder"
        android:label="fragment_make_order"
        tools:layout="@layout/fragment_make_order" >
        <argument
            android:name="name"
            app:argType="string" />

        <argument
            android:name="telephone"
            app:argType="string" />

        <argument
            android:name="adres"
            app:argType="string" />


        <argument
            android:name="savedItem"
            app:argType="string[]" />

        <action
            android:id="@+id/action_makeOrder_to_second"
            app:destination="@id/second" >
           <argument
                android:name="savedItem"
                app:argType="string[]" />


        </action>
    </fragment>



